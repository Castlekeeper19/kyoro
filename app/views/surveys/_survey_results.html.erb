    <div class="survey-show-chart">
        <h3><%= (question.category).split("_").join(" ") %> </h3>
        <%= column_chart question.user_answers.group(:answer_score).count, colors:["#F1807E", "#FFC65A", "#FCFC62", "#73BF8F", "#1EDD88"], width: "300px", height: "175px", min: 0, max: 6, xmin:1, xmax: 5, zeros: true%>
        <% question.user_answers.each do |answer| %>
          <% if answer.answer_score == 1 %>
       <p>Answered 1: <%= link_to answer.user.name, user_path(answer.user) %></p>
       <% end %>
       <% end %>
      </div>
